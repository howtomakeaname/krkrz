{
  "name": "krkrz",
  "version": "2.0.0",
  "dependencies": [
    "egl-registry",
    "opengl-registry",
    "glm",
    "libogg",
    "libpng",
    "libvorbis",
    "oniguruma",
    "opus",
    "opusfile",
    "picojson",
    "libjpeg-turbo",
    "zlib",
    {
      "name": "freetype",
      "features": [
        "zlib",
        "png",
        "bzip2"
      ]
    }
  ],
  "vcpkg-configuration": {
    "registries": [
      {
        "kind": "artifact",
        "location": "https://github.com/microsoft/vcpkg-ce-catalog/archive/refs/heads/main.zip",
        "name": "microsoft"
      }
    ]
  }
}
